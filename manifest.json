{
    "manifest_version": 3,
    "version": "1.1",
    "name": "Prodo",
    "action":{
        "default_popup": "practice.html",
        "default_icon": "picture.jpg",
        "scripts": ["content.js"]
    },

    "permissions":[
        "tabs",
        "storage",
        "activeTab"
    ],

    "background":{
        "service_worker":"background.js"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["practice.css"],
        "js": ["content.js", "background.js"]
    }]
      

}